s\" value=\"true\" append-icon=\"mdi-clipboard-list-outline\"></v-switch>\n          <v-switch v-model=\"interfacesSlider\" class=\"ma-4\" label=\"Interfaces\" value=\"true\" append-icon=\"mdi-console\"></v-switch>\n        </v-row>\n      </v-row>\n\n    <!-- Card for Thruster input fields -->\n      <v-card\n        class=\"mx-auto\"\n        max-width=\"400\"\n        outlined\n        raised\n      >\n        <v-form\n          ref=\"form\"\n        >\n        <v-container>\n          <p>Thruster</p>\n\n          <v-text-field\n            v-model=\"newSystem.name\"\n            :counter=\"20\"\n            label=\"Name\"\n            required\n          >\n          </v-text-field>\n\n          <v-text-field\n            v-model=\"newSystem.thrust\"\n            :counter=\"3\"\n            label=\"Thrust (mN)\"\n            required\n          >\n          </v-text-field>\n\n          <v-text-field\n            v-model=\"newSystem.specificImpulse\"\n            :counter=\"5\"\n            label=\"Specific Impulse (s)\"\n          >\n          </v-text-field>\n\n          <v-text-field\n            v-model=\"newSystem.manufacturer\"\n            :counter=\"20\"\n            label=\"Manufacturer\"\n            required\n          >\n          </v-text-field>\n\n          <v-text-field\n            v-model=\"newSystem.propellant\"\n            :counter=\"10\"\n            label=\"Propellant\"\n          >\n          </v-text-field>\n\n          <v-text-field\n            v-model=\"newSystem.weight\"\n            :counter=\"8\"\n            label=\"Weight (kg)\"\n          >\n          </v-text-field>\n\n          <v-text-field\n            v-model=\"newSystem.inputPower\"\n            :counter=\"5\"\n            label=\"Input Power (W)\"\n          >\n          </v-text-field>\n\n          </v-container>\n        </v-form>\n      </v-card>\n\n      <!-- Card for dimensions -->\n\n      <v-card v-if=\"dimensionsSlider\"\n        class=\"mx-auto\"\n        max-width