          }\n            }\n\n                },\n\n        login: function(type) {\n            if (type === 'admin'){\n\n                this.userType = \"admin\"\n            }\n                alert('Logging in as ' + type)\n            if (type === 'guest'){\n                this.$router.push({name: 'retrieve system'})\n            }\n\n            //eventBus.$emit('userLoggedIn', this.userType)\n            }\n        }\n    }\n</script>\n"]}]}